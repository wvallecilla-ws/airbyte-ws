{
  "streams": [
    {
      "stream": {
        "name": "audits",
        "json_schema": {
          "$schema": "http://json-schema.org/draft-04/schema#",
          "type": "object",
          "properties": {
            "count": {
              "type": "integer"
            },
            "total": {
              "type": "integer"
            },
            "audits": {
              "type": "array",
              "items": [
                {
                  "type": "object",
                  "properties": {
                    "audit_id": {
                      "type": "string"
                    },
                    "modified_at": {
                      "type": "string"
                    },
                    "template_id": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "audit_id",
                    "modified_at",
                    "template_id"
                  ]
                }
              ]
            }
          },
          "required": [
            "count",
            "total",
            "audits"
          ]
        },
        "supported_sync_modes": [
          "full_refresh",
          "incremental"
        ]
      },
      "sync_mode": "incremental",
      "destination_sync_mode": "overwrite"
    }
  ]
}